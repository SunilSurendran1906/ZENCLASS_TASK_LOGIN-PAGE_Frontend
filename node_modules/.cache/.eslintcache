gin_app\\client\\src\\components\\Profile.js",["81"],[],"import React, { useState } from \"react\";\r\nimport { Link, useNavigate } from \"react-router-dom\";\r\nimport avatar from \"../assets/profile.png\";\r\nimport toast, { Toaster } from \"react-hot-toast\";\r\nimport { useFormik } from \"formik\";\r\nimport useFetch from \"../hooks/fetchhook\";\r\nimport convertToBase64 from \"../helper/convert\";\r\nimport { profileValidation } from \"../helper/validate\";\r\nimport { updateUser } from \"../helper/helper\";\r\n\r\nimport styles from \"../styles/Username.module.css\";\r\nimport extend from \"../styles/profile.module.css\";\r\n\r\nexport default function Profile() {\r\n  const navigate = useNavigate();\r\n  const [file, setFile] = useState();\r\n  const [{ isLoading, apiData, serverError }] = useFetch();\r\n\r\n  const formik = useFormik({\r\n    initialValues: {\r\n      firstName: apiData?.firstName || \"\",\r\n      lastName: apiData?.lastName || \"\",\r\n      email: apiData?.email || \"\",\r\n      mobile: apiData?.mobile || \"\",\r\n      address: apiData?.address || \"\",\r\n    },\r\n    enableReinitialize: true,\r\n    validate: profileValidation,\r\n    validateOnBlur: false,\r\n    validateOnChange: false,\r\n    onSubmit: async (values) => {\r\n      values = await Object.assign(values, {\r\n        profile: file || apiData?.profile || \"\",\r\n      });\r\n      let updateUserPromsie = updateUser(values);\r\n      toast.promise(updateUserPromsie, {\r\n        loading: \"start Updating..!\",\r\n        success: <b>Updated Successfully..!</b>,\r\n        error: <b>Could Not Update!</b>,\r\n      });\r\n      // console.log(values);\r\n    },\r\n  });\r\n\r\n  /** formik doensn't support file upload so we need to create this handler */\r\n  const onUpload = async (e) => {\r\n    const base64 = await convertToBase64(e.target.files[0]);\r\n    setFile(base64);\r\n  };\r\n\r\n  // logOut handler function\r\n  function userLogOut() {\r\n    localStorage.removeItem(\"token\");\r\n    navigate(\"/\");\r\n  }\r\n\r\n  if (isLoading) return <h1 className=\"text-2xl font-bold\">isLoading</h1>;\r\n  if (serverError)\r\n    return <h1 className=\"text-xl text-red-500\">{serverError.message}</h1>;\r\n  return (\r\n    <div className=\"container mx-auto\">\r\n      <Toaster position=\"top-center\" reverseOrder={false}></Toaster>\r\n\r\n      <div className=\"flex justify-center items-center h-screen\">\r\n        <div\r\n          className={`${styles.glass} ${extend.glass}`}\r\n          style={{ width: \"45%\", paddingTop: \"3em\", height: \"95%\" }}\r\n        >\r\n          <div className=\"title flex flex-col items-center\">\r\n            <h4 className=\"text-5xl font-bold\">Profile</h4>\r\n            <span className=\"py-4 text-xl w-2/3 text-center text-gray-500\">\r\n              You can update the details\r\n            </span>\r\n          </div>\r\n\r\n          <form className=\"py-1\" onSubmit={formik.handleSubmit}>\r\n            <div className=\"profile flex justify-center py-4\">\r\n              <label htmlFor=\"profile\">\r\n                <img\r\n                  src={apiData?.profile || file || avatar}\r\n                  className={`${styles.profile_img} ${extend.profile_img}`}\r\n                  alt=\"avatar\"\r\n                />\r\n              </label>\r\n\r\n              <input\r\n                onChange={onUpload}\r\n                type=\"file\"\r\n                id=\"profile\"\r\n                name=\"profile\"\r\n              />\r\n            </div>\r\n            <div className=\"textbox flex flex-col items-center gap-6\">\r\n              <div className=\"name  flex w-3/4 gap-10\">\r\n                <input\r\n                  {...formik.getFieldProps(\"firstName\")}\r\n                  className={`${styles.textbox} ${extend.textbox}`}\r\n                  type=\"text\"\r\n                  placeholder=\"FirstName\"\r\n                />\r\n                <input\r\n                  {...formik.getFieldProps(\"lastName\")}\r\n                  className={`${styles.textbox} ${extend.textbox}`}\r\n                  type=\"text\"\r\n                  placeholder=\"LastName\"\r\n                />\r\n              </div>\r\n              <div className=\"name  flex w-3/4 gap-10\">\r\n                <input\r\n                  {...formik.getFieldProps(\"mobile\")}\r\n                  className={`${styles.textbox} ${extend.textbox}`}\r\n                  type=\"text\"\r\n                  placeholder=\"Moblie No.\"\r\n                />\r\n                <input\r\n                  {...formik.getFieldProps(\"email\")}\r\n                  className={`${styles.textbox} ${extend.textbox}`}\r\n                  type=\"text\"\r\n                  placeholder=\"Email*\"\r\n                />\r\n              </div>\r\n              <input\r\n                {...formik.getFieldProps(\"address\")}\r\n                className={styles.textbox}\r\n                type=\"text\"\r\n                placeholder=\"Address\"\r\n              />\r\n              <button className={styles.btn} type=\"submit\">\r\n                update Now\r\n              </button>\r\n            </div>\r\n            <div className=\"text-center py-4\">\r\n              <span className=\"text-gray-500\">\r\n                come back later?{\"  \"}\r\n                <button className=\"text-red-500\" onClick={userLogOut }>\r\n                  LogOut\r\n                </button>\r\n              </span>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","D:\\Nodejs\\BACKEND\\ZENCLASS_NODE_TASK\\RESTPASSWORD\\login_app\\client\\src\\components\\PageNoteFound.js",[],[],"D:\\Nodejs\\BACKEND\\ZENCLASS_NODE_TASK\\RESTPASSWORD\\login_app\\client\\src\\helper\\validate.js",[],["82","83"],"D:\\Nodejs\\BACKEND\\ZENCLASS_NODE_TASK\\RESTPASSWORD\\login_app\\client\\src\\helper\\convert.js",[],[],"D:\\Nodejs\\BACKEND\\ZENCLASS_